{
    "name": "siberfx/typesense-scout",
    "description": "Laravel Scout Driver for Typesense",
    "keywords": [
        "laravel",
        "typesense",
        "search"
    ],
    "type": "library",
    "homepage": "https://typesense.org",
    "license": "MIT",
    "authors": [
        {
            "name": "Selim Görmüş",
            "email": "info@siberfx.com",
            "homepage": "https://siberfx.com",
            "role": "Developer"
        },
        {
            "name": "siberfx",
            "homepage": "https://github.com/siberfx",
            "role": "Software Engineer"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Siberfx\\Typesense\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Siberfx\\Typesense\\TypesenseServiceProvider"
            ],
            "aliases": {
                "Typesense": "Siberfx\\Typesense\\TypesenseFacade"
            }
        }
    },
    "require": {
        "php": "^8.2",
        "laravel/scout": "^11.0|^12.0",
        "illuminate/bus": "^11.0|^12.0",
        "illuminate/contracts": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/pagination": "^11.0|^12.0",
        "illuminate/queue": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "typesense/typesense-php": "^5.0"
    },
    "config": {
        "platform": {
            "php": "8.3"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "suggest": {
        "typesense/typesense-php": "Required to use the Typesense php client."
    }
}
